{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Validators Up",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "count(up{job=~\"validator.*\"} == 1)",
          "legendFormat": "up"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 7 },
              { "color": "green", "value": 10 }
            ]
          },
          "unit": "none"
        }
      }
    },
    {
      "title": "Miner Up",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "targets": [
        {
          "expr": "count(up{job=\"miner\"} == 1)",
          "legendFormat": "up"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "unit": "none"
        }
      }
    },
    {
      "title": "Shares Stored (Total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "sum(djinn_validator_shares_stored_total)",
          "legendFormat": "total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      }
    },
    {
      "title": "Purchases (Total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(djinn_validator_purchases_processed_total)",
          "legendFormat": "total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "purple", "value": null }
            ]
          }
        }
      }
    },
    {
      "title": "Line Checks (Total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "sum(djinn_miner_checks_processed_total)",
          "legendFormat": "total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "orange", "value": null }
            ]
          }
        }
      }
    },
    {
      "title": "MPC Errors (24h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "targets": [
        {
          "expr": "sum(increase(djinn_validator_mpc_errors_total[24h]))",
          "legendFormat": "errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "title": "Request Rate (All Services)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum(rate(djinn_validator_requests_total[5m])) by (instance)",
          "legendFormat": "validator {{instance}}"
        },
        {
          "expr": "sum(rate(djinn_miner_requests_total[5m]))",
          "legendFormat": "miner"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Purchases by Result",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "sum(rate(djinn_validator_purchases_processed_total[5m])) by (result)",
          "legendFormat": "{{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "fillOpacity": 20 }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "available" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "unavailable" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "error" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "title": "Request Latency p95 (Validators)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(djinn_validator_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p95 {{endpoint}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "MPC Duration p95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(djinn_validator_mpc_duration_seconds_bucket[5m])) by (le, mode))",
          "legendFormat": "p95 {{mode}}"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(djinn_validator_mpc_duration_seconds_bucket[5m])) by (le, mode))",
          "legendFormat": "p50 {{mode}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Active Key Shares per Validator",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "djinn_validator_active_shares",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "title": "Outcomes Attested",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(djinn_validator_outcomes_attested_total[5m])) by (outcome)",
          "legendFormat": "{{outcome}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "fillOpacity": 20 }
        }
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["djinn", "overview"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Djinn Protocol Overview",
  "uid": "djinn-overview"
}
