{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Miner Status",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        { "expr": "up{job=\"miner\"}", "legendFormat": "status" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ]
        }
      }
    },
    {
      "title": "Line Checks (Total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        { "expr": "sum(djinn_miner_checks_processed_total)", "legendFormat": "total" }
      ],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "blue", "value": null }] } } }
    },
    {
      "title": "Lines Available vs Unavailable",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        { "expr": "sum(djinn_miner_lines_checked_total{result=\"available\"})", "legendFormat": "available" },
        { "expr": "sum(djinn_miner_lines_checked_total{result=\"unavailable\"})", "legendFormat": "unavailable" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "steps": [{ "color": "green", "value": null }] }
        }
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(djinn_miner_cache_operations_total{result=\"hit\"}[5m])) / (sum(rate(djinn_miner_cache_operations_total[5m])) > 0) * 100",
          "legendFormat": "hit_rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          }
        }
      }
    },
    {
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum(rate(djinn_miner_requests_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "title": "Request Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(djinn_miner_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p95 {{endpoint}}"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(djinn_miner_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p50 {{endpoint}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Lines Checked by Result",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "sum(rate(djinn_miner_lines_checked_total[5m])) by (result)",
          "legendFormat": "{{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "fillOpacity": 20 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "available" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "unavailable" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "title": "Odds API Queries",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "sum(rate(djinn_miner_odds_api_queries_total[5m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "fillOpacity": 20 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "success" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "cached" }, "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "error" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "title": "Cache Hit/Miss Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(djinn_miner_cache_operations_total[5m])) by (result)",
          "legendFormat": "{{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "fillOpacity": 30 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "hit" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "miss" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "title": "Proofs Generated by Type",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(djinn_miner_proofs_generated_total[5m])) by (type)",
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "fillOpacity": 20 } } }
    },
    {
      "title": "Circuit Breaker",
      "type": "state-timeline",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "djinn_miner_circuit_breaker_open",
          "legendFormat": "{{target}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Closed" }, "1": { "text": "Open" } } }
          ]
        }
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["djinn", "miner"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Djinn Miner",
  "uid": "djinn-miner"
}
