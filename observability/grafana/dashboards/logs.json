{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Log Volume",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({job=~\"$job\"} |~ \"$search\" [$__interval])) by (job)",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "custom": { "fillOpacity": 30, "stacking": { "mode": "normal" } } }
      }
    },
    {
      "title": "Error Logs",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 6 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{job=~\"$job\"} |~ \"error|ERROR|Error\" |~ \"$search\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none",
        "enableLogDetails": true
      }
    },
    {
      "title": "All Logs",
      "type": "logs",
      "gridPos": { "h": 16, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{job=~\"$job\"} |~ \"$search\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none",
        "enableLogDetails": true
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["djinn", "logs"],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "custom",
        "query": "validator,miner",
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "includeAll": true,
        "multi": true
      },
      {
        "name": "search",
        "type": "textbox",
        "current": { "text": "", "value": "" },
        "label": "Search"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Djinn Logs",
  "uid": "djinn-logs"
}
